{
  "name": "@bytepulse/pulsewave-client",
  "version": "0.3.1",
  "description": "PulseWave Client SDK - React hooks and components for WebRTC video/audio",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./components/styles.css": "./dist/components/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bytepulseflow/pulsewave.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/bytepulseflow/pulsewave#readme",
  "bugs": {
    "url": "https://github.com/bytepulseflow/pulsewave/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && cp -r src/components/styles.css dist/components/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "@bytepulse/pulsewave-shared": "workspace:*",
    "mediasoup-client": "^3.7.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "author": {
    "name": "Kaushal Mehta",
    "url": "https://github.com/kaushald4"
  }
}
